  object qrVendaRecibo: TZQuery
    Connection = DM.ZConnection
    OnCalcFields = qrVendaReciboCalcFields
    SQL.Strings = (
      
        'select v.idvenda, v.datahora, v.total_venda, v.codcliente, v.des' +
        'conto, v.acrescimo,'
      
        'c.nome, c.documento, c.endereco, c.bairro, c.cidade, c.uf, c.tel' +
        'efone, c.celular, c.rg, c.cep, c.email,'
      'u.nome as responsavel'
      'from tbvenda v'
      'left join tbcliente c on v.codcliente = c.codcliente '
      'left join tbusuario u on v.usuario = u.idtbusuario'
      'where v.idvenda = :pidvenda')
