  object qrLucratividade: TZQuery
    Connection = DM.ZConnection
    OnCalcFields = qrLucratividadeCalcFields
    SQL.Strings = (
      'SELECT i.idvenda, v.datahora, v.desconto, v.acrescimo, '
      
        'sum(i.preco * i.qtde) as valor_total, sum(i.desconto * i.qtde) a' +
        's desconto_item, '
      'sum(i.preco_custo * i.qtde) as preco_custo FROM tbvenda_item i'
      'inner join tbvenda v on i.idvenda = v.idvenda'
      '/*where*/'
      'group by i.idvenda'
      'order by i.idvenda')
