  object qrClienteProduto: TZQuery
    Connection = DM.ZConnection
    SQL.Strings = (
      
        'select p.idproduto, p.codbarras, p.descricao, p.valor_custo, p.d' +
        't_cadastro, c.idcliente, cl.nome, c.preco, c.edital, p.idcategor' +
        'ia, t.descricao as categoria, p.tamanho from tbproduto p'
      'left join tbcategoria t on p.idcategoria = t.idcategoria'
      'left join tbcliente_preco c on c.idproduto = p.idproduto'
      'left join tbcliente cl on c.idcliente = cl.codcliente '
      'where p.idproduto > 0 '
      '')
    Params = <>
    Left = 608
    Top = 40
  end
