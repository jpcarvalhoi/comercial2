  object qrClienteVenda: TZQuery
    Connection = DM.ZConnection
    SQL.Strings = (
      
        'select sum(ni.qtde) as qtde, p.descricao, c.nome as prefeitura, ' +
        'cl.nome as cliente, '
      'cl.status_abo as tipo_sanguineo, p.tamanho from tbnota_item ni'
      'inner join tbnota n on ni.idnota = n.idnota'
      'inner join tbproduto p on ni.idproduto = p.idproduto'
      'inner join tbcliente c on n.idcliente = c.codcliente '
      'inner join tbcliente cl on n.idcliente_venda = cl.codcliente'
      'where ni.idnota > 0'
      '/*where*/'
      
        'group by ni.qtde, p.descricao, c.nome, cl.nome, cl.status_abo, p' +
        '.tamanho')
