  object qrNota: TZQuery
    Connection = DM.ZConnection
    OnCalcFields = qrNotaCalcFields
    SQL.Strings = (
      'SELECT i.*, n.*, '
      
        'c.nome as cliente, c.documento, c.endereco, c.bairro, c.cidade, ' +
        'c.uf, c.cep, c.telefone, c.celular, c.email, c.fantasia, c.codga' +
        'rra, '
      
        'f.cnpj as documento_for, f.razao as cliente_for, f.endereco as e' +
        'ndereco_for, f.bairro as bairro_for, f.cidade as cidade_for, f.u' +
        'f as uf_for,'
      
        'f.cep as cep_for, f.tel as telefone_for, f.tel2 as celular_for, ' +
        'f.email as email_for,'
      'p.est_atual, u.nome, '
      
        'if(i.idkit is null, p.codbarras, k.codbarras) as codbarras , if(' +
        'i.idkit is null, p.descricao, k.descricao) as descricao,'
      '(coalesce(i.qtde,0) * coalesce(i.valor_unit,0)) as valor_total,'
      
        '(coalesce(n.total_itens,0) + coalesce(n.acrescimo,0) - coalesce(' +
        'n.desconto,0)) as valor_nota,'
      
        'cv.nome as vcliente, cv.documento as vdocumento, cv.endereco as ' +
        'vendereco, cv.bairro as vbairro, cv.codgarra as vcodgarra, '
      
        'cv.cidade as vcidade, cv.uf as vuf, cv.cep as vcep, cv.telefone ' +
        'as vtelefone, cv.celular as vcelular,'
      
        'cv.email as vemail, p.tamanho, cv.fantasia as vfantasia, cv.matr' +
        'icula as vmatricula, cv.status_abo as vstatus_abo '
      'FROM tbnota_item i'
      'inner join tbnota n on i.idnota = n.idnota'
      'left join tbproduto p on i.idproduto = p.idproduto'
      'left join tbkit k on i.idkit = k.idkit'
      'left join tbfornecedor f on n.idfornecedor = f.idfornecedor'
      'left join tbusuario u on n.usuario = u.idtbusuario'
      'left join tbcliente c on n.idcliente = c.codcliente'
      'left join tbcliente cv on n.idcliente_venda = cv.codcliente'
      'left join tbcategoria ct on p.idcategoria = ct.idcategoria'
      'where n.tipo_nota = :ptipo_nota and n.idnota = :pidnota'
      'order by i.idnota, ct.descricao, p.descricao')
    Params = <
      item
        DataType = ftString
        Name = 'ptipo_nota'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'pidnota'
        ParamType = ptInput
      end>
    Left = 624
    Top = 184
    ParamData = <
      item
        DataType = ftString
        Name = 'ptipo_nota'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'pidnota'
        ParamType = ptInput
      end>
