  object qrItemRecibo: TZQuery
    Connection = DM.ZConnection
    SQL.Strings = (
      
        'select i.idvenda, i.iditem, i.idproduto, i.qtde, i.preco, i.desc' +
        'onto, i.idkit,'
      
        'if(i.idproduto is not null, p.codbarras, k.codbarras) as codbarr' +
        'as,'
      
        'if(i.idproduto is not null, p.descricao, k.descricao) as descric' +
        'ao,'
      '((i.qtde * i.preco) - i.desconto) as valor_total'
      'from tbvenda_item i'
      'left join tbproduto p on i.idproduto = p.idproduto'
      'left join tbkit k on i.idkit = k.idkit'
      'where i.idvenda = :pidvenda')
