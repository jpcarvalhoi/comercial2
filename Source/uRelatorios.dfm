  object qrProducao: TZQuery
    Connection = DM.ZConnection
    SQL.Strings = (
      
        'SELECT i.idproduto, p.descricao, p.tamanho, p.codbarras, sum(i.q' +
        'tde) as qtde FROM tbnota_item i'
      'inner join tbproduto p on i.idproduto = p.idproduto'
      'inner join tbnota n on i.idnota = n.idnota'
      'left join tbcategoria ct on p.idcategoria = ct.idcategoria'
      'where i.iditem > 0 /*where*/'
      'group by i.idproduto, p.descricao, p.tamanho, p.codbarras '
      'order by ct.descricao, p.descricao'
      '')
